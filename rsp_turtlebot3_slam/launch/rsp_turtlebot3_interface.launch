<launch>
    <include file="$(find rsp_loop_detection)/launch/rsp_loop_detection.launch" />

    <node
        name="rsp_frontier_explorer_service"
        pkg="rsp_wf_frontier_exploration"
        type="rsp_wf_frontier_exploration_node"
        output="screen"></node>

    <!-- robot action node -->
    <node 
        name="rsp_random_walk_exploration_node"
        pkg="rsp_random_walk_exploration" 
        type="randomWalk_node"  
        output="screen"></node>

    <node
        name="deployer"
        pkg="rtt_ros"
        type="deployer"
        args="-s $(find rsp_turtlebot3_slam)/scripts/turtlebot3.ops"
        output="screen"></node>
</launch>